{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": [
      "ES2019",
      "DOM"
    ],
    "module": "ES2015",
    "moduleResolution": "Node",
    "noEmit": true,
    "rootDir": "./e2e",
    "strict": true,
    "target": "ES2015",
    "types": [
      "cypress",
      "node"
    ]
  },
  "include": [
    "./e2e/**/*.ts"
  ]
}