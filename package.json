{
  "name": "calystone",
  "version": "0.67.0",
  "license": "MIT",
  "description": "self management system based on your file system",
  "keywords": [
    "local-first",
    "todo",
    "file-system"
  ],
  "type": "module",
  "main": "main/index.html",
  "repository": {
    "type": "git",
